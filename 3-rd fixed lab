//1 part task 10
            int[] a = new int[10];

            int count = 0;
           
            Console.Write(" введите индекс P: ");

            int p = int.Parse(Console.ReadLine());
            Console.Write("введите индекс Q: ");
            int q = int.Parse(Console.ReadLine());
            if ((p < q) || (p > q))
            {




                for (int i = 0; i < a.Length; i++)
                {
                    Console.Write($"Элемент с индексом {i} равен: ");
                    a[i] = int.Parse(Console.ReadLine());
                }
                if (a[p] < a[q] )
                {

                    for (int i = p + 1; i < q; i++)
                    {
                        count++;
                    }
                }
                else
                {
                    Console.WriteLine("Неправильно ");
                }
            }
            else
            {
                Console.WriteLine("неверно");

            }
            Console.WriteLine(count);
            Console.ReadLine();
            
            
            
              //2 часть 10 номер
            int kol = int.Parse(Console.ReadLine());
            int[] a = new int[kol];
            for (int i = 0; i < kol; i++)
            {
                a[i] = int.Parse(Console.ReadLine());
                if (a[i] < 0)
                {
                    Console.WriteLine("элементы должны быть положительными");
                    break;
                }
            }
            for (int i = 0; i < kol; i++)
            {
                int imax = i;
                int amax = a[i];
                for (int j = i + 1; j < kol; j++)
                {
                    if (a[j] > a[i])
                    {
                        amax = a[i];
                        imax = j;

                    }
                }
                a[imax] = a[i];
                a[i] = amax;
            }
            a[a.Length - 1] = 0;
            for (int i = 0; i < kol - 1; i++)
                Console.Write("{0:d} ", a[i]);
            Console.ReadLine();
            
            
            
             //3 часть 9 номер
            int kol_Vo = int.Parse(Console.ReadLine());
            int[] posl = new int[kol_Vo];

            int len_posl = 1;
            int max_len_posl = 0;

            for (int i = 0; i < posl.Length; i++)
            {
                Console.Write($"Элемент с индексом {i} равен: ");
                posl[i] = int.Parse(Console.ReadLine());
            }

            for (int i = 0; i < posl.Length - 1; i++)
            {

                if (posl[i] < posl[i + 1])
                {

                    len_posl += 1;
                    if (len_posl > max_len_posl)
                    {
                        max_len_posl = len_posl;


                    }
                }
                else
                {
                    len_posl = 1;
                }

            }






            Console.Write($"Самая большая последовательность:  {max_len_posl} ");
            Console.ReadLine();
            
            
            
             //3 часть 13 номер
            int n = int.Parse(Console.ReadLine());
            int[] a = new int[n];
            int min;
            int i_x;
            int count = 0;
            for (int i = 0; i < n; i++)
            {
                Console.Write($"Значение элемента с индексом {i}: ");
                a[i] = int.Parse(Console.ReadLine());
            }


            for (int i = 0; i < a.Length; i++)
            {
                for (int j = i + 1; j < a.Length; j++)
                {
                    if (a[i] == a[j])
                    {
                        a[j] = 0;
                        count++;
                    }
                }
            }
            for (int i = 0; i < a.Length - 1; i++)
            {
                min = a[i];
                i_x = i;
                for (int j = i + 1; j < a.Length; j++)
                {
                    if (a[j] > min)
                    {
                        min = a[j];
                        i_x = j;
                    }

                }
                a[i_x] = a[i];
                a[i] = min;

            }
            for (int i = 0; i < a.Length ; i++)

                Console.Write("{0:d} ", a[i]);
            Console.ReadLine();
            
            
            
              //3 часть 12 номер
            int[] a = new int[12];
            int imax;
            int amax;
            int count = 0;

            for (int i = 0; i < 12; i++)
            {
                Console.Write($"Значение элемента с индексом {i}: ");
                a[i] = int.Parse(Console.ReadLine());
            }
            for (int i = 0; i < a.Length - 1; i++)
            {
                amax = a[i];
                imax = i;
                for (int j = i + 1; j < a.Length; j++)
                {
                    if (a[j] > amax)
                    {
                        amax = a[j];
                        imax = j;
                    }

                }
                a[imax] = a[i];
                a[i] = amax;

            }


            for (int i = 0; i < a.Length; i++)
            {
                if (a[i] < 0)
                {
                    a[i] = 0;
                    count += 1;

                }
            }

            for (int i = 0; i < a.Length - count; i++)

                Console.Write("{0:d} ", a[i]);
            Console.ReadLine();
            
            
            
            
